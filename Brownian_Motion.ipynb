   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "One dimensional brownian motion. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "dt = 0.01\n",
    "N = 100\n",
    "D = 1.0\n",
    "\n",
    "sigma = np.sqrt(2*D*dt)\n",
    "\n",
    "Y = np.random.randn(N-1)*sigma\n",
    "X = np.zeros(N)\n",
    "X[1:] = np.cumsum(Y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [
